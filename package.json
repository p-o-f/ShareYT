{
  "name": "ShareYT",
  "description": "A free, cross-platform browser extension that allows you to easily share and react to YouTube videos with your friends.",
  "private": true,
  "version": "0.0.6",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,js,md,mdx,css,yaml}\"",
    "prettier:format": "prettier --write \"**/*.{ts,tsx,js,md,mdx,css,yaml}\""
  },
  "dependencies": {
    "@webext-core/messaging": "^2.3.0",
    "firebase": "^12.2.1",
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^6.4.0",
    "firebase-tools": "^14.14.0",
    "js-sha256": "^0.11.1",
    "lucide-react": "^0.543.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "webextension-polyfill": "^0.12.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/chrome": "^0.1.27",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@wxt-dev/auto-icons": "^1.1.0",
    "@wxt-dev/module-react": "^1.1.3",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.2.0",
    "prettier": "^3.6.2",
    "publish-browser-extension": "^4.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "wxt": "^0.20.11"
  }
}
